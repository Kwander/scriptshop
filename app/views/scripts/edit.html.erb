<h1>Edit page of <%= @script.title %> </h2>

<%= form_for @script do |f| %>
<p><%= f.label :title %>: <%= f.text_field :title %></p>
<p><%= f.label :description %>: <%= f.text_area :description %></p>
<p><%= f.label :price %>: <%= f.text_area :price %></p>
<p><%= f.submit "Save changes" %></p>

<% end %>